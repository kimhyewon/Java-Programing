package first;

public class Person {
	public int age;
	public int height;
	private float weight;
	private String name;
	
	public Person(){}
	
	public Person(String pName) {
		name = pName;
	}
	
	public String getName() {
		return name;
	}
	
	public void setWeight(float w) {
		weight = w;
	}
	
	public float getWeight() {
		return weight;
	}
}
