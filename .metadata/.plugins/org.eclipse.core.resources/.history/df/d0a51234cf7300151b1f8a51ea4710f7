		this.day = day;
	}
	
	public void setMonth() {
		this.month = month;
	}
	
	public void setYear() {
		this.year = year;
	}
	
	public boolean checkDay() {
		if(day)
	}
	
	public boolean checkMonth() {
		
	}
	
	public boolean checkYear() {
		if(year >= 0 && year <= 2015) {
			return true;
		}
		return false;
	}
	
	public boolean checkValidate() {
		return false;
	}
}
