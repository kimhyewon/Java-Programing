package first;
//윤년 : 4로 나누어지면서 100으로 나누어 떨어지지 않는 년

public class BirthDay {
	private int day;
	private int month;
	private int year;
	
	public BirthDay(int day, int month, int year) {
		this.day = day;
		this.month = month;
		this.year = year;
	}
	
